<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
  <title>HOOKAH BUTLER</title>

  <meta name="viewport" content="width=device-width"/>
  <% attachments.inline['app-store-btn.png'] = File.read("#{Rails.root}/app/assets/images/app-store-btn.png") %>
  <% attachments.inline['banner.jpg'] = File.read("#{Rails.root}/app/assets/images/banner.jpg") %>
  <% attachments.inline['google-play-btn.png'] = File.read("#{Rails.root}/app/assets/images/google-play-btn.png") %>
  <% attachments.inline['logo.png'] = File.read("#{Rails.root}/app/assets/images/logo.png") %>
  <%# attachments.inline['product_image.png'] = File.read("#{Rails.root}/app/assets/images/product_image.png") %>
  <% attachments.inline['sep.png'] = File.read("#{Rails.root}/app/assets/images/sep.png") %>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>
  <style type="text/css">

      #outlook a {
          padding: 0;
      }

      body {
          width: 100% !important;
          -webkit-text-size-adjust: 100%;
          -ms-text-size-adjust: 100%;
          margin: 0;
          padding: 0;
          background: #ebebeb !important;
          color: #000 !important;
          font-family: Arial, 'Helvetica Neue', Helvetica, sans-serif !important;
      }

      .ExternalClass {
          width: 100%;
      }

      .ExternalClass, .ExternalClass p, .ExternalClass span, .ExternalClass font, .ExternalClass td, .ExternalClass div {
          line-height: 100%;
      }

      #backgroundTable {
          margin: 0;
          padding: 0;
          width: 100% !important;
          line-height: 100% !important;
      }

      img {
          outline: none;
          text-decoration: none;
          -ms-interpolation-mode: bicubic;
          border: none;
      }

      a img {
          border: none;
      }

      .image_fix {
          display: block;
      }

      p {
          margin: 1em 0;
      }

      h1, h2, h3, h4, h5, h6 {
          color: #fcbf31 !important;
      }

      h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {
          color: #99cc33 !important;
      }

      h1 a:active, h2 a:active, h3 a:active, h4 a:active, h5 a:active, h6 a:active {
          color: #99cc33 !important;
      }

      h1 a:visited, h2 a:visited, h3 a:visited, h4 a:visited, h5 a:visited, h6 a:visited {
          color: purple !important;
      }

      table td {
          border-collapse: collapse;
      }

      table {
          border-collapse: collapse;
          mso-table-lspace: 0pt;
          mso-table-rspace: 0pt;
      }

      a {
          color: orange;
      }

      ::-webkit-input-placeholder {
          color: #49545d;
      }

      :-moz-placeholder {
          color: #49545d;
      }

      ::-moz-placeholder {
          color: #49545d;
      }

      :-ms-input-placeholder {
          color: #49545d;
      }

      span a {
          color: #99cc33 !important;
      }

      @media only screen and (max-device-width: 480px) {
          a[href^="tel"], a[href^="sms"] {
              text-decoration: none;
              color: #99cc33;
              pointer-events: none;
              cursor: default;
          }

          .mobile_link a[href^="tel"], .mobile_link a[href^="sms"] {
              text-decoration: default;
              color: #99cc33 !important;
              pointer-events: auto;
              cursor: default;
          }
      }

      @media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {
          a[href^="tel"], a[href^="sms"] {
              text-decoration: none;
              color: #99cc33;
              pointer-events: none;
              cursor: default;
          }

          .mobile_link a[href^="tel"], .mobile_link a[href^="sms"] {
              text-decoration: default;
              color: #99cc33 !important;
              pointer-events: auto;
              cursor: default;
          }

          #em-bg {
              width: 600px !important;
          }
      }

      * {
          margin: 0;
          padding: 0;
      }

      * {
          font-family: Arial, sans-serif;
      }

      .collapse {
          margin: 0;
          padding: 0;
      }

      body {
          -webkit-font-smoothing: antialiased;
          -webkit-text-size-adjust: none;
          width: 100% !important;
          height: 100%;
          margin: 0 auto;
      }

      table.head-wrap {
          width: 100%;
      }

      table.body-wrap {
          width: 100%;
      }

      .container {
          clear: both !important;
          display: block !important;
          height: 803px;
          margin: 0 auto !important;
          max-width: 600px !important;
      }

      .content {
          max-width: 100%;
          margin: 0 auto;
          display: block !important;
          padding: 13px 18px;
      }

      .content table {
          width: 100%;
      }

      .clear {
          display: block;
          clear: both;
      }

      td {
          -webkit-box-sizing: border-box;
          -moz-box-sizing: border-box;
          box-sizing: border-box;
      }

      img {
          width: 100%;
      }

      @media only screen and (max-width: 600px) {

          div[class="column"] {
              width: auto !important;
              float: none !important;
          }

          table.social div[class="column"] {
              width: auto !important;
          }

      }
  </style>

</head>

